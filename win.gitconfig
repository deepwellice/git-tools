[merge]
	tool = p4merge
[diff]
	tool = p4merge
[difftool "p4merge"]
	path = C:\\Program Files\\Perforce\\p4merge.exe
[mergetool "p4merge"]
	path = C:\\Program Files\\Perforce\\p4merge.exe
[credential]
	helper = manager
[credential "https://dev.azure.com"]
	usehttppath = true
[safe]
	directory = D:/git/fusion
	directory = D:/git/fusion/dev
[include]
	path = ./.common.gitconfig
[http]
	sslverify = false
